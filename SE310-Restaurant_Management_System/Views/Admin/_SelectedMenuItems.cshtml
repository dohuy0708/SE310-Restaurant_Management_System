@using SE310_Restaurant_Management_System.ViewModels

@model List<MenuItemViewModel>

@foreach (var item in Model)
{
    <tr>
        <td style="width: 50%">@item.Name</td>
        <td style="width: 20%">@item.Price.ToString("N0")đ</td>
        <td style="width: 10%">
            <button type="button" class="btn btn-danger btn-sm remove-item-btn" data-id="@item.MenuItemID">Xóa</button>
        </td>
    </tr>
}